#include "main.h"

/**
*Custom printf function that handles specific conversion specifiers.
*@param format A format string containing conversion specifiers.
*@return The number of characters printed.
*/

int main() {
	unsigned int u_value = 42;
	unsigned int o_value = 42;
	unsigned int x_value = 42;

	printf("Unsigned Decimal (u): %u\n", u_value);

	printf("Octal (o): %o\n", o_value);

	printf("Lowercase Hexadecimal (x): %x\n", x_value):

	printf("Uppercase Hexadecimal (X): %X\n", x_value);

	return 0;
}
